<?xml version="1.0" encoding="UTF-8"?>

<odoo>

    <data>

        <!-- record create: form -->
        <record id="vf_ceid_form" model="ir.ui.view">

            <field name="name">vf_ceid_form</field>
            <field name="model">ceid.list</field>
            <field name="arch" type="xml">

                <form>
                    <sheet>

                        <div class="oe_button_box" name="button_box">

                            <button type="object" name="action_show_is_ch"
                              class="oe_stat_button" icon="fa-book" string="is ch">
                            </button>
                        </div>

                        <group>
                            <field name="name"/>
                        </group>

                    </sheet>
                </form>

            </field>

        </record>

        <!-- action (form & tree) -->
        <record id="vf_ceid_action" model="ir.actions.act_window">
            <field name="name">vf ceid</field>
            <field name="res_model">ceid.list</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0),
    (0, 0, {'view_mode': 'tree', 'view_id': ref('fame_dss.ceid_list_tree')}),
    (0, 0, {'view_mode': 'form', 'view_id': ref('fame_dss.vf_ceid_form')})]"/>
        </record>

    </data>

        <!-- menu item app: name and action -->
        <menuitem id="menu_vf_ceid"
            name="vf ceid"
            action="vf_ceid_action"
            sequence="100"
        />

</odoo>