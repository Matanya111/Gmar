<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_ceid_graph" model="ir.ui.view">
            <field name="name">ceid.graph</field>
            <field name="model">execl.1</field>
            <field name="arch" type="xml">
                <graph string="report" sample="1">
                    <field name="WW"/>
                    <field name="ceid_list"/>
                    <field name="AVAILABILITY"/>
                </graph>
            </field>
    </record>
    <record id="view_ceid_pivot" model="ir.ui.view">
            <field name="name">ceid.pivot</field>
            <field name="model">execl.1</field>
            <field name="arch" type="xml">
                <pivot string="report" sample="1">
                    <field name="WW"/>
                    <field name="ceid_list"/>
                    <field name="AVAILABILITY"/>
                </pivot>
            </field>
    </record>
</odoo>
