<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_ceid_avail" model="ir.ui.view">
            <field name="name">ceid.graph</field>
            <field name="model">execl.1</field>
            <field name="arch" type="xml">
                <graph string="report" sample="1">
                    <field name="WW" type="row"/>
                    <field name="AVAILABILITY" type="measure"/>
                </graph>
            </field>
    </record>

    <record id="view_ch_avail" model="ir.ui.view">
            <field name="name">ceid.graph</field>
            <field name="model">execl.1</field>
            <field name="arch" type="xml">
                <graph string="report" sample="1">
                    <field name="CHAMBER" type="row"/>
                    <field name="AVAILABILITY" type="measure"/>
                </graph>
            </field>
    </record>

    <record id="view_4_13_weeks" model="ir.ui.view">
            <field name="name">view.4.13.weeks</field>
            <field name="model">ceid.list</field>
            <field name="arch" type="xml">
                <group>
                        <field name="last_4_weeks_avail" type="row"/>
                        <field name="last_13_weeks_avail" type="row"/>
                </group>
            </field>
    </record>

    <record id="view_ceid_dt" model="ir.ui.view">
            <field name="name">ceid.graph</field>
            <field name="model">execl.2</field>
            <field name="arch" type="xml">
                <graph string="report" sample="1">
                    <field name="STATUS" type="row"/>
                    <field name="TIME" type="measure"/>
                </graph>
            </field>
    </record>

    <record id="view_ch_dt" model="ir.ui.view">
            <field name="name">ceid.graph</field>
            <field name="model">execl.2</field>
            <field name="arch" type="xml">
                <graph string="report" sample="1">
                    <field name="CHAMBER" type="row"/>
                    <field name="STATUS" type="row"/>
                    <field name="TIME" type="measure"/>
                </graph>
            </field>
    </record>

</odoo>
