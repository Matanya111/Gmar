<?xml version="1.0" encoding="UTF-8"?>
<odoo>
        <record id="restrictive_tool_action" model="ir.actions.act_window">
            <field name="name">restrictive tools</field>
            <field name="res_model">ceid.list</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="fame_dss.ceid_list_tree"/>
            <field name="context">{ 'tree_view_ref':'ceid_list_tree'}</field>
            <field name="domain">[('goal', '>', 4)]</field> 
        </record>

        <!-- menu item app: name and action -->
        <menuitem id="menu_restrictive_tool"
            name="restrictive tool"
            action="restrictive_tool_action"
            sequence="100"
        />
</odoo>